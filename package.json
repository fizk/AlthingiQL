{
  "name": "AlthingiQL",
  "version": "1.0.0",
  "description": "GraphQL server and React/Apollo client",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "$(npm bin)/forever start ./dist/server/index.js",
    "stop": "$(npm bin)/forever stop ./dist/server/index.js",
    "build": " babel --extensions \".tsx,.ts\" --out-dir ./dist ./src/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fizk/AlthingiQL"
  },
  "dependencies": {
    "apollo-client": "^1.9.3",
    "bootstrap": "^4.1.1",
    "d3-scale": "^2.0.0",
    "d3-shape": "^1.2.0",
    "d3-time": "^1.0.8",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "forever": "^0.15.3",
    "graphql": "^0.13.2",
    "graphql-iso-date": "^3.5.0",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.49",
    "@babel/plugin-transform-react-jsx": "^7.0.0-beta.49",
    "@babel/preset-es2015": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/preset-stage-2": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@storybook/react": "^3.4.7",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^23.1.0",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-strict-value": "^1.0.4",
    "stylelint-scss": "^3.1.0",
    "ts-loader": "^3.5.0",
    "typescript": "^2.9.1",
    "webpack": "^3.11.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react",
      "@babel/preset-es2015",
      [
        "@babel/preset-stage-2",
        {
          "decoratorsLegacy": true
        }
      ]
    ],
    "plugins": [
      "babel-plugin-ts-props",
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-transform-react-jsx",
      [
        "babel-plugin-transform-require-ignore",
        {
          "extensions": [
            ".css",
            ".scss"
          ]
        }
      ],
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
